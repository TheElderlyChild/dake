<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.application.dake">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dake">
        <activity android:name=".views.SignUpPasswordActivity"></activity>
        <activity android:name=".views.SignUpNameActivity" />
        <activity android:name=".views.SignUpPhoneNoActivity" />
        <activity android:name=".views.AuthActivity" />
        <activity android:name=".views.MapActivity" />
        <activity android:name=".views.RestaurantPreview" />
        <activity android:name=".views.MenuActivity" />
        <activity android:name=".views.AccountActivity" />
        <activity android:name=".views.SearchActivity" />
        <activity android:name=".views.HomePage" />
        <activity android:name=".views.SignUpDetails" />
        <activity android:name=".views.SignUpEmailActivity" />
        <activity android:name=".views.LoginActivity" />
        <activity android:name=".views.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>